"*":
  core:
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "*.pyc"
    ]
    projectHome: "/Users/{{ lookup('env', 'USER') }}/Workspace"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  editor:
    fontSize: 13
    invisibles: {}
    showInvisibles: true
    tabLength: 4
  "exception-reporting":
    userId: "afec2d02-1342-5ebe-aef0-e530cac0dd8d"
  minimap:
    plugins:
      "git-diff": true
      "git-diffDecorationsZIndex": 0
  "spell-check":
    grammars: [
      "text.html.basic"
    ]
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: false
    removeTrailingWhitespace: false
